{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "REACT_APP_BACKEND_URL": "https://portfolio-backend-9ttg.onrender.com",
      "REACT_APP_ENABLE_VISUAL_EDITS": "false",
      "ENABLE_HEALTH_CHECK": "false"
    }
  }
}